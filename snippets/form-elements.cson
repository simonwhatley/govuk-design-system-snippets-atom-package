    """
  'Select':
    'leftLabel': 'GOV.UK'
    'description': 'Select – GOV.UK Design System'
    'descriptionMoreURL': 'https://design-system.service.gov.uk/components/select/'
    'prefix': 'govuk-select'
    'body': """
    {{ govukSelect({
