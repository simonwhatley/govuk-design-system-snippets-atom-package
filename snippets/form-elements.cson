    """
  'Telephone number':
    'leftLabel': 'GOV.UK'
    'description': 'Telephone number – GOV.UK Design System'
    'descriptionMoreURL': 'https://design-system.service.gov.uk/patterns/telephone-numbers/'
    'prefix': 'govuk-telephone'
    'body': """
